<?xml version="1.0" encoding="UTF-8"?>

<templates class="array">

    <!--图片管理开始-->
    <template key="searchPictures.list">
        <requestUrl>/admin/searchPictures/list</requestUrl>
        <templateRequestType>POST</templateRequestType>
    </template>
    <template key="searchPictures.listData">
        <requestUrl>/admin/searchPictures/listData</requestUrl>
        <loading type="boolean">true</loading>
        <targetSelector>#fbMappingListData</targetSelector>
        <templateRequestType>POST</templateRequestType>
    </template>
    <template key="searchPictures.loadAdd">
        <requestUrl>/admin/searchPictures/loadAdd</requestUrl>
        <targetSelector>#addModalData</targetSelector>
        <templateRequestType>POST</templateRequestType>
    </template>
     <!--图片管理结束-->
     
     <!-- 权限管理开始 -->
    <template key="userRole.list">
        <requestUrl>/admin/userRole/list</requestUrl>
        <loading type="boolean">true</loading>
        <templateRequestType>POST</templateRequestType>
    </template>
    <template key="userRole.listData">
        <requestUrl>/admin/userRole/listData</requestUrl>
        <loading type="boolean">true</loading>
        <targetSelector>#fbMappingListData</targetSelector>
        <templateRequestType>POST</templateRequestType>
    </template>
    <template key="userRole.modify">
        <requestUrl>/admin/userRole/modify</requestUrl>
        <templateRequestType>POST</templateRequestType>
    </template>
    <template key="userRole.loadModify">
        <requestUrl>/admin/userRole/loadModify</requestUrl>
        <targetSelector>#modifyModalData</targetSelector>
        <templateRequestType>POST</templateRequestType>
    </template>
    <!--权限管理相关 结束-->

    <!--条件查询 开始-->
    <template key="insectSearch.list">
        <requestUrl>/admin/insectSearch/list</requestUrl>
        <loading type="boolean">true</loading>
        <templateRequestType>POST</templateRequestType>
    </template>
    <template key="insectSearch.listData">
        <requestUrl>/admin/insectSearch/listData</requestUrl>
        <loading type="boolean">true</loading>
        <targetSelector>#fbMappingListData</targetSelector>
        <templateRequestType>POST</templateRequestType>
    </template>
    <template key="insectSearch.loadAdd">
        <requestUrl>/admin/insectSearch/loadAdd</requestUrl>
        <targetSelector>#addModalData</targetSelector>
        <templateRequestType>POST</templateRequestType>
    </template>
    <template key="insectSearch.modify">
        <requestUrl>/admin/insectSearch/modify</requestUrl>
        <templateRequestType>POST</templateRequestType>
    </template>
    <template key="insectSearch.loadModify">
        <requestUrl>/admin/insectSearch/loadModify</requestUrl>
        <targetSelector>#modifyModalData</targetSelector>
        <templateRequestType>POST</templateRequestType>
    </template>
    
    <!--前后台映射关系相关 结束-->


	<template key="categoryMapping.list">
		<requestUrl>/admin/categoryMapping/list</requestUrl>
		<templateRequestType>POST</templateRequestType>
	</template>
	<template key="categoryMapping.listData">
		<requestUrl>/admin/categoryMapping/listData</requestUrl>
		<targetSelector>#categoryMappingsData</targetSelector>
		<templateRequestType>POST</templateRequestType>
	</template>

    <!--类目树管理 开始-->
    <template key="insectCategory.tree">
        <requestUrl>/admin/insectCategory/tree</requestUrl>
        <loading type="boolean">true</loading>
    </template>

    <template key="pestCategory.tree">
        <requestUrl>/admin/pestCategory/tree</requestUrl>
        <loading type="boolean">true</loading>
    </template>
    <!--类目树管理 结束-->
    
    <!-- 前台系统 -->
    <template key="user.index">
    	<requestUrl>/index</requestUrl>
        <loading type="boolean">true</loading>
    </template>

</templates>
